#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
 int n,i=0,j,temp,b;
  cin>>n;
  int a[n];
  while(std::cin>>a[i++]);
    while(std::cout<<a[--n]<<' ' && n); 
  j=n-1;
  while(i<j)
  {
      swap(a[i],a[j]);
      i++,j--;
  }
  for(i=0;i<n;i++)
  {
      printf("%d ",&a[i]);
  }   
    return 0;
}
 
